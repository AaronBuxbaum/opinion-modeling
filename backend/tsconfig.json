{
  "compilerOptions": {
    "strict": true,
    "target": "es2018",
    "module": "CommonJS",
    "lib": [
      "esnext"
    ],
    "rootDir": ".",
    "plugins": [
      {
        "name": "nexus/typescript-language-service"
      }
    ],
    "typeRoots": [
      "node_modules/@types",
      "../node_modules/@types",
      "types"
    ],
    "noEmit": true
  },
  "include": [
    ".",
    "types.d.ts"
  ],
}
